<body style="background-color: #000;">


    <script>
        {{#if err}}
        
            Swal.fire({
                icon: 'error',
                title: '{{title}}',
                html: "{{{text}}}",
                confirmButtonText: 'OK',
                width: 600,
                padding: '1em',
                color: '#716add',
                background: '#fff url(/images/trees.png)',
                backdrop: `
                    rgba(0,0,123,0.4)
                    url('https://thumbs.gfycat.com/DiligentWastefulGavial-max-1mb.gif')
                    left top
                    no-repeat
                    `
            }).then((result) => {
                        /* Read more about isConfirmed, isDenied below */
                        if (result.isConfirmed) {
                            document.getElementById("{{action}}").click()
                        }
                    })
        
        {{/if}}

                {{#if suc}}
                    Swal.fire({
                        icon: 'success',
                        title: '{{title}}',
                        html: "{{{text}}}",
                        width: 600,
                        padding: '2em',
                        color: '#716add',
                        background: '#fff url(/images/trees.png)',
                        backdrop: `
                    rgba(0,0,123,0.4)
                    url('https://media.giphy.com/media/CaS9NNso512WJ4po0t/giphy.gif')
                    left top
                    no-repeat
                    `
                    })
                

                {{/if}}


    </script>

